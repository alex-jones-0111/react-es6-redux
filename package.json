{
  "name": "react-es6-redux",
  "version": "2.5.0",
  "description": "A simple app to try React / ES6 & redux, using topheman-apis-proxy as data api backend",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test",
    "unit-test": "mocha --compilers js:babel-core/register --recursive --require ./test/unit/setup.js",
    "unit-test-watch": "npm run lint-watch & npm run unit-test -- --watch",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx src test",
    "lint-watch": "./node_modules/.bin/esw --watch --ext .js --ext .jsx src test",
    "dev": "echo 'deprecated, use npm run webpack-dev' && npm run webpack-dev",
    "dev-mock": "echo 'deprecated, use npm run webpack-mock' && npm run webpack-mock",
    "build": "./node_modules/.bin/webpack --progress && ./node_modules/.bin/gulp build",
    "build-prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress -p && NODE_ENV=production ./node_modules/.bin/gulp build",
    "build-prod-owner": "API_ROOT_URL='https://topheman-apis-proxy.herokuapp.com/github' npm run build-prod",
    "build-prod-all": "npm run build-prod && DEVTOOLS=true NODE_ENV=production npm run build",
    "build-prod-all-owner": "API_ROOT_URL='https://topheman-apis-proxy.herokuapp.com/github' npm run build-prod-all",
    "serve-build": "cd build && ../node_modules/.bin/serve --port 9001",
    "webpack-lazy": "./node_modules/.bin/webpack-dev-server --progress --colors --lazy",
    "webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
    "webpack-dev": "API_ROOT_URL='http://localhost:8000/github' DEVTOOLS=true npm run webpack",
    "webpack-mock": "DEVTOOLS=true NODE_ENV=mock API_ROOT_URL='http://localhost' npm run webpack",
    "webpack-dev-simple": "DEVTOOLS=true npm run webpack",
    "generate-http-fixtures": "node ./bin/nock/call-nock.js"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/topheman/react-es6-redux.git"
  },
  "keywords": [
    "react",
    "ES6"
  ],
  "author": "Christophe Rosset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topheman/react-es6-redux/issues"
  },
  "homepage": "https://github.com/topheman/react-es6-redux",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "del": "^2.0.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.2",
    "eslint-watch": "^2.1.4",
    "expect": "^1.13.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "git-rev-sync": "^1.4.0",
    "gulp": "^3.9.0",
    "gulp-footer": "^1.0.5",
    "gulp-if": "^2.0.0",
    "gulp-inject": "^3.0.0",
    "gulp-minify-html": "^1.0.4",
    "gulp-processhtml": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.6",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "load-grunt-tasks": "^3.3.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "nock": "^3.3.2",
    "node-sass": "^3.3.3",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "run-sequence": "^1.1.4",
    "sass-loader": "^3.0.0",
    "serve": "^1.4.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "es6-promise": "^3.0.2",
    "history": "~1.13.1",
    "lscache": "^1.0.5",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "redux-router": "^1.0.0-beta3",
    "superagent": "^1.4.0",
    "superagent-mocker": "^0.3.0"
  },
  "private": true
}
